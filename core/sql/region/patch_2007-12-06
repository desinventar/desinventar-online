# Patch to fix Regions databases
alter table DatabaseInfo add GeoLimitMinX DOUBLE NULL;
alter table DatabaseInfo add GeoLimitMinY DOUBLE NULL;
alter table DatabaseInfo add GeoLimitMaxX DOUBLE NULL;
alter table DatabaseInfo add GeoLimitMaxY DOUBLE NULL;

alter table Disaster change EventId EventId VARCHAR(50);
alter table Disaster change CauseId CauseId VARCHAR(50);

alter table GeoLevel change GeoLevelLayer GeoLevelLayerFile VARCHAR(50);
alter table GeoLevel add GeoLevelLayerCode VARCHAR(50);
alter table GeoLevel add GeoLevelLayerName VARCHAR(50);
