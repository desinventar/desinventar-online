{
  "smarty": {
    "cachedir": "/tmp"
  },
  "paths": {
    "fonts": "/usr/share/fonts/liberation/fonts.txt",
    "libs_url": "//cdn-desinventar.inticol.com/js"
  },
  "maps": {
    "mapserver_uri": "/cgi-bin/mapserv",
    "mapserver": "mapserv",
    "proj_lib": "/usr/share/proj",
    "worldmap_dir": "/var/local/desinventar/worldmap"
  },
  "flags": {
    "mode": "normal",
    "env": "online",
    "arch": "LINUX",
    "debug": false,
    "auto_login": 0,
    "general_secure_login": 0
  },
  "logger": {
    "file": "php://stdout",
    "level": 100
  },
  "database": {
    "driver": "sqlite",
    "db_host": "127.0.0.1",
    "db_port": "",
    "db_user": "",
    "db_passwd": "",
    "db_dir": "/var/local/desinventar/db"
  },
  "test": {
    "url": "http://localhost:8081",
    "username": "root",
    "passwd": "desinventar",
    "database": "TEST"
  }
