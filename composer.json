{
  "name": "desinventar/online",
  "description": "Inventory system of the effects of disasters",
  "require": {
    "smarty/smarty": "^3.1",
    "aura/sqlquery": "^2.7",
    "desinventar/jpgraph": "^3.0.7",
    "aura/sql": "^2",
    "monolog/monolog": "^1.23",
    "slim/slim": "^3.0",
    "pimple/pimple": "~3.0",
    "ramsey/uuid": "^3.7",
    "aura/session": "^2.1",
    "fostam/getopts": "^1.1",
    "koenster/php-language-detection": "^1.0",
    "cweagans/composer-patches": "~1.0",
    "phpstan/phpstan": "^0.10.3"
  },
  "require-dev": {
    "ccampbell/chromephp": "^4.1",
    "phpmd/phpmd": "^2.6",
    "guzzlehttp/guzzle": "^6.3",
    "squizlabs/php_codesniffer": "*",
    "phpunit/phpunit": "^8.0.0"
  },
  "repositories": [
    {
      "url": "https://github.com/desinventar/jpgraph",
      "type": "git"
    }
  ],
  "authors": [
    {
      "email": "jhcaiced@inticol.com",
      "homepage": "http://www.desinventar.org",
      "name": "Jhon H. Caicedo",
      "role": "Project lead / Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "DesInventar\\": [
        "src/DesInventar"
      ],
      "Api\\": [
        "src/Api"
      ],
      "Test\\": [
        "tests/unit"
      ]
    }
  },
  "keywords": [
    "disasters",
    "inventories",
    "risk"
  ],
  "extra": {
    "patches": {
      "desinventar/jpgraph": {
        "jpgraph patches for PHP7": "files/patches/jpgraph_php7_compatibility.diff"
      }
    }
  }
}
